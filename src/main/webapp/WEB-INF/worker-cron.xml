<cronentries>

	<cron>
		<url>/pratilipi/process?cron</url>
		<target>worker</target>
		<schedule>every 5 minutes from 8:00 to 20:00</schedule>
		<timezone>Asia/Calcutta</timezone>
	</cron>

	<cron>
		<url>/pratilipi/backup?cron</url>
		<target>worker</target>
		<schedule>every 30 minutes from 8:00 to 20:00</schedule>
		<timezone>Asia/Calcutta</timezone>
	</cron>

	<cron>
		<url>/pratilipi/idf?cron</url>
		<target>worker</target>
		<schedule>every 6 hours from 8:00 to 20:00</schedule>
		<timezone>Asia/Calcutta</timezone>
	</cron>


	<cron>
		<url>/author/process?cron</url>
		<target>worker</target>
		<schedule>every 5 minutes from 8:00 to 20:00</schedule>
		<timezone>Asia/Calcutta</timezone>
	</cron>
	
	<cron>
		<url>/author/backup?generateCsv=true&cron</url>
		<target>worker</target>
		<schedule>every 1 hours from 8:00 to 20:00</schedule>
		<timezone>Asia/Calcutta</timezone>
	</cron>
	
	
	<cron>
		<url>/user/backup?generateCsv=true&cron</url>
		<target>worker</target>
		<schedule>every 1 hours from 8:00 to 20:00</schedule>
		<timezone>Asia/Calcutta</timezone>
	</cron>

</cronentries>